{
  "name": "fingerprint-pro-server-api-openapi",
  "private": true,
  "version": "3.0.0",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "webpack-dev-server --open",
    "build": "webpack",
    "lintSchema": "vacuum lint ./schemas/*.yaml -d --errors --base schemas --ignore-file vacuum-ignore.yaml",
    "lintSchemaDist": "vacuum lint ./dist/schemas/*.yaml -d --errors --base dist/schemas --ignore-file vacuum-ignore.yaml",
    "validateSchema": "tsx ./bin/validateSchema.ts",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:dts": "echo 'No typescript tests' && exit 0",
    "lint": "eslint --ext .js,.ts --ignore-path .gitignore --max-warnings 0 .",
    "lint:fix": "pnpm lint --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "pnpm prettier . --write",
    "changeset": "node scripts/changeset.js",
    "changeset-version": "node scripts/zipChangesets.js && pnpm exec changeset version"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.6",
    "@fingerprintjs/changesets-changelog-format": "^0.2.0",
    "@fingerprintjs/conventional-changelog-dx-team": "^0.1.0",
    "@fingerprintjs/eslint-config-dx-team": "^0.1.0",
    "@fingerprintjs/fingerprintjs-pro-server-api": "^6.6.0",
    "@fingerprintjs/prettier-config-dx-team": "^0.1.0",
    "@fingerprintjs/tsconfig-dx-team": "^0.0.2",
    "@quobix/vacuum": "^0.18.5",
    "@types/express": "^5.0.3",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.14.9",
    "@types/prompts": "^2.4.9",
    "@types/swagger-ui": "^3.52.4",
    "ajv": "^8.16.0",
    "ajv-draft-04": "^1.0.0",
    "ajv-formats": "^2.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^6.11.0",
    "dotenv": "^16.4.5",
    "express": "^5.1.0",
    "glob": "^11.0.0",
    "html-webpack-plugin": "^5.6.4",
    "human-id": "^4.1.1",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "json-loader": "^0.5.7",
    "playwright": "^1.45.1",
    "prompts": "^2.4.2",
    "style-loader": "^3.3.4",
    "swagger-ui": "^5.28.0",
    "ts-loader": "^9.5.4",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.2.2",
    "yaml": "^2.4.5",
    "yaml-loader": "^0.8.1",
    "zip-lib": "^1.0.4",
    "znv": "^0.4.0",
    "zod": "^3.23.8"
  }
}
